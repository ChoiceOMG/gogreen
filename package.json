    "prisma:init": "dotenv -e .env.local -- npx prisma init",
    "prisma:deploy": "dotenv -e .env.local -- npx prisma migrate deploy",
    "prisma:seed": "dotenv -e .env.local -- npx prisma db seed ",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio",
    "prisma:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "prisma:reset": "dotenv -e .env.local -- npx prisma migrate reset --force",
    "prisma:push": "dotenv -e .env.local -- npx prisma db push",
    "dev": "next dev -p 3047",
    "build": "next build",
    "start": "next start -p 3047",
    "lint": "next lint"
  },
